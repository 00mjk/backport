{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "no-console": 0,
    "import/no-commonjs": 0,
    "arrow-body-style": 0
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "prettier"]
}
